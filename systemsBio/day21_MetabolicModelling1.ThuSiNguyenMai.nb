Cell[TextData[{
 StyleBox["Q2.4: ",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 StyleBox["Study how van Heerden\[CloseCurlyQuote]s ",
  FontWeight->"Bold"],
 StyleBox["et al. ",
  FontWeight->"Bold",
